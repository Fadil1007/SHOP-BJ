{# @var ea \EasyCorp\Bundle\EasyAdminBundle\Context\AdminContext #}
{% extends '@EasyAdmin/page/content.html.twig' %}

{% block content_title %}
    {% if pageName is defined %}
        {{ pageName }}
    {% else %}
        {{ parent() }}
    {% endif %}
{% endblock %}

{% block main %}
    {% if content is defined %}
        {{ content|raw }}
    {% else %}
        {{ parent() }}
    {% endif %}
{% endblock %}